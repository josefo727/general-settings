{
    "name": "josefo727/general-settings",
    "description": "General settings module for Laravel projects.",
    "type": "library",
    "authors": [
        {
            "name": "Jos√© Rafael Gutierrez",
            "email": "josefo727@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "orchestra/testbench": "^8.5",
        "nesbot/carbon": "^2.54"
    },
    "autoload": {
        "psr-4": {
            "Josefo727\\GeneralSettings\\": "src/"
        },
        "classmap": [
            "config/"
        ],
        "directories": [
            "lang",
            "resources/views",
            "vendor/josefo727/general-settings/resources/views"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Josefo727\\GeneralSettings\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.2"
        },
        "laravel": {
            "providers": [
                "Josefo727\\GeneralSettings\\Providers\\GeneralSettingsServiceProvider"
            ],
            "aliases": {
                "GeneralSetting": "Josefo727\\GeneralSettings\\Facades\\GeneralSetting"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=config --force",
            "php artisan vendor:publish --tag=lang --force",
            "php artisan vendor:publish --tag=resources --force"
        ]
    }
}
